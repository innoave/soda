
# EditorConfig is awesome: http://EditorConfig.org

root = true

[*]
charset = utf-8
end_of_line = lf

[*.{scala,sbt,xml}]
indent_style = space
indent_size = 2
insert_final_newline = true
trim_trailing_whitespace = true

[*.{css,json}]
indent_style = tab
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true
